components:
  parameters:
    User-Agent:
      name: User-Agent
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: 'Mozilla/5.0 (X11; Ubuntu; Linux x86_64; rv:90.0) Gecko/20100101 Firefox/90.0'
    Accept:
      name: Accept
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: 'application/json, text/plain, */*'
    Accept-Language:
      name: Accept-Language
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: 'en-US,en;q=0.5'
    Referer:
      name: Referer
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: 'https://app.karza.in/vkyc/login'
    x-product:
      name: x-product
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: videokyc
    Content-type:
      name: Content-type
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: application/json
    Origin:
      name: Origin
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: 'https://app.karza.in'
    Connection:
      name: Connection
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: keep-alive
    Cookie:
      name: Cookie
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: _ga=GA1.2.970318156.1627887049; _gid=GA1.2.1353084182.1627887049; 
    Sec-Fetch-Dest:
      name: Sec-Fetch-Dest
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: empty
    Sec-Fetch-Mode:
      name: Sec-Fetch-Mode
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: cors
    Sec-Fetch-Site:
      name: Sec-Fetch-Site
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: cross-site
    x-karza-key:
      name: x-karza-key
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: C7IIRnnxrrwByFuz
    karzatoken:
      name: karzatoken
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJyZXF1ZXN0X2lkIjoiZDlkNDljNzItN2FjMS00OTQ0LTg1NzQtZGUwNmU1OGZjNGY2IiwidXNlcl9pZCI6NTMwNzMyLCJzY29wZSI6WyJ2aWRlb19reWMiXSwiZW52IjoidGVzdCIsImNsaWVudF9pZCI6IlN1cGVyTW9uZXlfZDhsNnBIIiwic3RhZ2UiOiJ0ZXN0IiwidXNlcl90eXBlIjoib3BlbiIsImV4cGlyeV90aW1lIjoiMDQtMDgtMjAyMVQwNjoyMzo1OCJ9.U_WemrovLwfLoW5GO21aElHE9nz2eSo0fAeZnXuHCMs
    Content-Type:
      name: Content-Type
      in: header
      required: true
      style: simple
      schema:
        type: string
        example: application/json
