      responses:
        '200':
          description: ''
          headers: {}
      requestBody:
        required: true
        content:
          text/plain:
            schema:
              type: string
              example:
                username: supermoney_u1
                password: Karza@1234
            example: '{"username":"supermoney_u1","password":"Karza@1234"}'
  /v3/get-jwt:
    post:
      summary: Generate Session Token
      operationId: GenerateSessionToken
      parameters:
        - $ref: '#/components/parameters/x-karza-key'
